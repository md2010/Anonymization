<Window x:Class="Anonyimization.RecordSuppressionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Anonyimization"
        mc:Ignorable="d"
        Title="RecordSuppressionWindow" Height="450" Width="800">
    <Grid Margin="0,0,0,-6">
        <DataGrid Name="dataGrid" ItemsSource="{Binding}" Margin="10,10,10,99"/>
        <Button Content="Obriši" Click="ButtonDelete_Click" HorizontalAlignment="Left" Margin="76,358,0,0" VerticalAlignment="Top" Width="98" Height="40" FontSize="14" Background="#FFC3A8A8"/>
        <Button Content="Spremi" Click="ButtonSave_Click" HorizontalAlignment="Left" Margin="646,358,0,0" VerticalAlignment="Top" Width="98" Height="40" FontSize="14" Background="#FFA9CFB0"/>

    </Grid>
</Window>
