<Window x:Class="Anonyimization.FinishWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Anonyimization"
        mc:Ignorable="d"
        Title="FinishWindow" Height="450" Width="800">
    <Grid Background="Azure">
        <DataGrid Name="dataGrid" Margin="0,10,322,26" ItemsSource="{Binding data}"/>
        <Button Content="Izvezi CSV dokument" Click="ButtonExport_Click" HorizontalAlignment="Left" Margin="570,0,0,0" VerticalAlignment="Center" Height="45" Width="159" FontSize="16" Background="#FFA9CFB0"/>

    </Grid>
</Window>
