<Window x:Class="Anonyimization.PermutationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Anonyimization"
        mc:Ignorable="d"
        Title="PermutationWindow" Height="450" Width="800">
    <Grid>
        <ComboBox Name="comboBox" ItemsSource="{Binding Attributes}" SelectedItem="{Binding SelectedAttribute}" HorizontalAlignment="Left" Margin="140,191,0,0" VerticalAlignment="Top" Width="140" Height="35"/>
        <Label Content="Odaberi atribut" HorizontalAlignment="Left" Margin="10,185,0,0" VerticalAlignment="Top" Width="130" FontSize="16"/>
        <DataGrid Name="dataGrid" Margin="305,10,10,94"/>
        <Button Content="Primjeni" Click="ButtonApply_Click" HorizontalAlignment="Left" Margin="75,269,0,0" VerticalAlignment="Top" Height="39" Width="107" Background="#FFC3A8A8"/>
        <Button Content="Spremi" Click="ButtonSave_Click" HorizontalAlignment="Left" Margin="623,368,0,0" VerticalAlignment="Top" Height="39" Width="111" Background="#FFA9CFB0"/>
    </Grid>
</Window>
